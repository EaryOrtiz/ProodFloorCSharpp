                                            <div class="progress-bar bg-white" role="progressbar" style="width: @monthHydro.TotalEfficiency%;"
                                                 aria-valuenow="@monthHydro.TotalEfficiency" aria-valuemin="0" aria-valuemax="100"></div>
                                        </div>
                                        <span>Completados: @monthHydro.TestJobsCounted </span>
                                    </div>
