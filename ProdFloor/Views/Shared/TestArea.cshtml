                                                 aria-valuenow="@lastMonthHydro.TotalEfficiency" aria-valuemin="0" aria-valuemax="100"></div>
                                        </div>
                                        <span>Completados: @lastMonthHydro.TestJobsCounted </span>
                                    </div>
                                </div>

                            </div>
                        </div>


                    </div>
                </div>
                <a class="carousel-control-prev" href="#carouselEFFData" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselEFFData" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
