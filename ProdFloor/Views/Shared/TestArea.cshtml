@model ProdFloor.Models.ViewModels.Report.ReportsViewModel
@{
    ViewData["Title"] = "ProdFloor Stats";
    <script>
        $(document).ready(function () {
            setInterval(function () {
                location.reload(true);
            }, 60000);

        });
    </script>
    DailyReport dailyM2000 = Model.dailyReports.FirstOrDefault(m => m.JobTypeName == "M2000");
    DailyReport dailyHydro = Model.dailyReports.FirstOrDefault(m => m.JobTypeName == "ElmHydro");
    DailyReport dailyM4000 = Model.dailyReports.FirstOrDefault(m => m.JobTypeName == "M4000");
    DailyReport dailyTraction = Model.dailyReports.FirstOrDefault(m => m.JobTypeName == "ElmTract");

    DailyReport weekM2000 = Model.WeekReports.FirstOrDefault(m => m.JobTypeName == "M2000");
    DailyReport weekHydro = Model.WeekReports.FirstOrDefault(m => m.JobTypeName == "ElmHydro");
    DailyReport weekM4000 = Model.WeekReports.FirstOrDefault(m => m.JobTypeName == "M4000");
    DailyReport weekTraction = Model.WeekReports.FirstOrDefault(m => m.JobTypeName == "ElmTract");

    DailyReport monthM2000 = Model.MonthReports.FirstOrDefault(m => m.JobTypeName == "M2000");
    DailyReport monthHydro = Model.MonthReports.FirstOrDefault(m => m.JobTypeName == "ElmHydro");
    DailyReport monthM4000 = Model.MonthReports.FirstOrDefault(m => m.JobTypeName == "M4000");
    DailyReport monthTraction = Model.MonthReports.FirstOrDefault(m => m.JobTypeName == "ElmTract");

    DailyReport lastMonthM2000 = Model.PastMonthReports.FirstOrDefault(m => m.JobTypeName == "M2000");
    DailyReport lastMonthHydro = Model.PastMonthReports.FirstOrDefault(m => m.JobTypeName == "ElmHydro");
    DailyReport lastMonthM4000 = Model.PastMonthReports.FirstOrDefault(m => m.JobTypeName == "M4000");
    DailyReport lastMonthTraction = Model.PastMonthReports.FirstOrDefault(m => m.JobTypeName == "ElmTract");
}

<div>
    <div class="row mb-1 pb-1">
        @Html.Partial("TestSlidesDuo", Model)
    </div>
    <div class="row mt-1 pt-1 pl-5">
        <div class="col-12 mr-auto">
            <div id="carouselEFFData" class="carousel slide" data-ride="carousel" data-interval="15000" style="width: 90rem;">
                <div class="carousel-inner">
                    <div class="carousel-item active">

                        <div class="card text-white bg-success" style="height: 7rem">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-2 pl-0 border-right text-center">
                                        <h2>@dailyM4000.JobTypeName</h2>
                                    </div>
                                    <div class="col font-weight-bold border-right" style="font-size: 18px">
                                        <span>@dailyM4000.TodayDate</span><br />
                                        <span>Eficiencia:  @dailyM4000.TotalEfficiency%</span>
                                        <div class="progress bg-secondary" style="height: 0.2rem;width: 12rem">
                                            <div class="progress-bar bg-white" role="progressbar" style="width: @dailyM4000.TotalEfficiency%;"
                                                 aria-valuenow="@dailyM4000.TotalEfficiency" aria-valuemin="0" aria-valuemax="100"></div>
                                        </div>
                                        <span>Completados: @dailyM4000.TestJobsCounted </span>
                                    </div>
                                    <div class="col font-weight-bold border-right" style="font-size: 18px">
                                        <span>En esta semana</span><br />
                                        <span>Eficiencia:  @weekM4000.TotalEfficiency%</span>
                                        <div class="progress bg-secondary" style="height: 0.2rem;width: 12rem">
                                            <div class="progress-bar bg-white" role="progressbar" style="width: @weekM4000.TotalEfficiency%;"
                                                 aria-valuenow="@weekM4000.TotalEfficiency" aria-valuemin="0" aria-valuemax="100"></div>
                                        </div>
                                        <span>Completados: @weekM4000.TestJobsCounted </span>
                                    </div>
                                    <div class="col font-weight-bold border-right" style="font-size: 18px">
                                        <span>En este mes</span><br />
                                        <span>Eficiencia:  @monthM4000.TotalEfficiency%</span>
                                        <div class="progress bg-secondary" style="height: 0.2rem;width: 12rem">
                                            <div class="progress-bar bg-white" role="progressbar" style="width: @monthM4000.TotalEfficiency%;"
                                                 aria-valuenow="@monthM4000.TotalEfficiency" aria-valuemin="0" aria-valuemax="100"></div>
                                        </div>
                                        <span>Completados: @monthM4000.TestJobsCounted </span>
                                    </div>
                                    <div class="col font-weight-bold" style="font-size: 18px">
                                        <span>En el mes pasado</span><br />
                                        <span>Eficiencia:  @lastMonthM4000.TotalEfficiency%</span>
                                        <div class="progress bg-secondary" style="height: 0.2rem;width: 12rem">
                                            <div class="progress-bar bg-white" role="progressbar" style="width: @lastMonthM4000.TotalEfficiency%;"
                                                 aria-valuenow="@lastMonthM4000.TotalEfficiency" aria-valuemin="0" aria-valuemax="100"></div>
                                        </div>
                                        <span>Completados: @lastMonthM4000.TestJobsCounted </span>
                                    </div>
                                </div>

                            </div>
                        </div>

                    </div>
                    <div class="carousel-item">

                        <div class="card text-white bg-danger" style="height: 7rem">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-2 pl-0 border-right text-center">
                                        <h3>Element Traction</h3>
                                    </div>
