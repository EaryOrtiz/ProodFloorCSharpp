@model ProdFloor.Models.ViewModels.TestJob.TestJobSearchViewModel
<div id="accordionSearch">
    <div class="card">
        <div class="card-header bg-dark text-center">
            <div class="btn-group" style="float:left">
                <!--Search and Clean-->
                <button class="btn btn-dark" style="border-color:transparent" type="submit" value="Search"><i class="fa fa-search" aria-hidden="true"></i></button>
                <button class="btn btn-dark" style="border-color:transparent" name="CleanFields" type="submit" value="true"><i class="fas fa-undo"></i></button>
            </div>
            <div class="btn-group">
                <!--Header JobInfo-->
                <button type="button" id="headingJobInfo" class="btn btn-dark" style="border-color:transparent" data-toggle="collapse" data-target="#collapseJobInfo" aria-expanded="true" aria-controls="collapseJobInfo">
                    <h6 class="mb-0 text-center"> Job Information</h6>
                </button>
                <!--Header TestJob-->
                <button type="button" id="headingTestJobInfo" class="btn btn-dark" style="border-color:transparent" data-toggle="collapse" data-target="#collapseTestJobInfo" aria-expanded="true" aria-controls="collapseTestJobInfo">
                    <h6 class="mb-0 text-center"> Test Job</h6>
                </button>
                <!--Header TestFeatures-->
                <button type="button" id="headingTestFeatures" class="btn btn-dark" style="border-color:transparent" data-toggle="collapse" data-target="#collapseTestFeatures" aria-expanded="true" aria-controls="collapseTestFeatures">
                    <h6 class="mb-0 text-center"> Test Features</h6>
                </button>
            </div>

        </div>

        <!--Body Job Info-->
        <div id="collapseJobInfo" class="collapse show" aria-labelledby="headingJobInfo" data-parent="#accordionSearch">
            <div class="card-body">
                <div class="row">
                    <div class="col-3" >
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="JobName" class="col-sm-4 col-form-label"></label>
                            <input type="text" class="form-control form-control-sm col-sm-7" asp-for="JobName" data-val="@false">
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="JobNum" class="col-sm-4 col-form-label"></label>
                            <input type="text" class="form-control form-control-sm col-sm-7" asp-for="JobNum" data-val="@false">
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="HoistWayData.LandingSystemID" class="col-sm-4 col-form-label"></label>
                            <landing-sys-select class="form-control-sm form-control col-sm-7" asp-for="HoistWayData.LandingSystemID"></landing-sys-select>
                        </div>
                    </div>
                    <div class="col-3" >
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="Canada" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.Canada" class="form-control-sm form-control col-sm-3" asp-for="Canada" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="Ontario" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.Ontario" class="form-control-sm form-control col-sm-3" asp-for="Ontario" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="Manual" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.Manual" class="form-control-sm form-control col-sm-3" asp-for="Manual" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="MOD" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.MOD" class="form-control-sm form-control col-sm-3" asp-for="MOD" select-for="boolSearch"></custom-select>
                        </div>
                    </div>
                    <div class="col-3" >
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="MView" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.MView" class="form-control-sm form-control col-sm-3" asp-for="MView" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="IMonitor" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.IMonitor" class="form-control-sm form-control col-sm-3" asp-for="IMonitor" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="Duplex" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.Duplex" class="form-control-sm form-control col-sm-3" asp-for="Duplex" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="SHC" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.SHC" class="form-control-sm form-control col-sm-3" asp-for="SHC" select-for="boolSearch"></custom-select>
                        </div>
                    </div>
                    <div class="col-3" >
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="EDGELS" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.EDGELS" class="form-control-sm form-control col-sm-3" asp-for="EDGELS" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="RailLS" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.RailLS" class="form-control-sm form-control col-sm-3" asp-for="RailLS" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="HAPS" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.HAPS" class="form-control-sm form-control col-sm-3" asp-for="HAPS" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="TwosStarters" class="col-sm-4 col-form-label"></label>
                            <custom-select selected-value="@Model.TwosStarters" class="form-control-sm form-control col-sm-3" asp-for="TwosStarters" select-for="boolSearch"></custom-select>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--Body TestJob Info-->
        <div id="collapseTestJobInfo" class="collapse" aria-labelledby="headingTestJobInfo" data-parent="#accordionSearch">
            <div class="card-body">
                <div class="row">
                    <div class="col-3">
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="TestJob.SinglePO" class="col-sm-4 col-form-label"></label>
                            <input type="text" class="form-control form-control-sm col-sm-7" asp-for="TestJob.SinglePO">
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="TestJob.TechnicianID" class="col-sm-4 col-form-label"></label>
                            <input type="text" class="form-control form-control-sm col-sm-7" asp-for="TestJob.TechnicianID">
                        </div>

                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="TestJob.StationID" class="col-sm-4 col-form-label"></label>
                            <station-select class="form-control form-control-sm col-7" asp-for="TestJob.StationID"></station-select>
                        </div>
                    </div>
                    <div class="col-3" >
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="TestJob.JobLabel" class="col-sm-4 col-form-label"></label>
                            <custom-select  class="form-control-sm form-control col-7" asp-for="TestJob.JobLabel" select-for="Label"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="TestJob.Status" class="col-sm-4 col-form-label"></label>
                            <custom-select  class="form-control-sm form-control col-sm-7" asp-for="TestJob.Status" select-for="TestJobStatus"></custom-select>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--Body TestFeatures Info-->
        <div id="collapseTestFeatures" class="collapse" aria-labelledby="headingTestFeatures" data-parent="#accordionSearch">
            <div class="card-body">
                <div class="row">
                    <div class="col-3" style="padding-right:0px;margin-right:0px;padding-left:0px;margin-left:0px">
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="Overlay" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.Overlay" class="form-control-sm form-control col-sm-3" asp-for="Overlay" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="Group" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.Group" class="form-control-sm form-control col-sm-3" asp-for="Group" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="PC" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.PC" class="form-control-sm form-control col-sm-3" asp-for="PC" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="BrakeCoilVoltageMoreThan10" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.BrakeCoilVoltageMoreThan10" class="form-control-sm form-control col-sm-3" asp-for="BrakeCoilVoltageMoreThan10" select-for="boolSearch"></custom-select>
                        </div>
                    </div>
                    <div class="col-3" style="padding-right:0px;margin-right:0px;padding-left:0px;margin-left:0px">
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="EMBrake" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.EMBrake" class="form-control-sm form-control col-sm-3" asp-for="EMBrake" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="EMCO" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.EMCO" class="form-control-sm form-control col-sm-3" asp-for="EMCO" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="R6" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.R6" class="form-control-sm form-control col-sm-3" asp-for="R6" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="Local" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.Local" class="form-control-sm form-control col-sm-3" asp-for="Local" select-for="boolSearch"></custom-select>
                        </div>
                    </div>
                    <div class="col-3" style="padding-right:0px;margin-right:0px;padding-left:0px;margin-left:0px">
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="ShortFloor" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.ShortFloor" class="form-control-sm form-control col-sm-3" asp-for="ShortFloor" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="Custom" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.Custom" class="form-control-sm form-control col-sm-3" asp-for="Custom" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="MRL" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.MRL" class="form-control-sm form-control col-sm-3" asp-for="MRL" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="CTL2" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.CTL2" class="form-control-sm form-control col-sm-3" asp-for="CTL2" select-for="boolSearch"></custom-select>
                        </div>
                    </div>
                    <div class="col-3" style="padding-right:0px;margin-right:0px;padding-left:0px;margin-left:0px">
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="TrajetaCPI" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.TrajetaCPI" class="form-control-sm form-control col-sm-3" asp-for="TrajetaCPI" select-for="boolSearch"></custom-select>
                        </div>
                        <div class="form-group row" style="margin-bottom:0px;">
                            <label asp-for="Cartop" class="col-sm-6 col-form-label"></label>
                            <custom-select selected-value="@Model.Cartop" class="form-control-sm form-control col-sm-3" asp-for="Cartop" select-for="boolSearch"></custom-select>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

</div>