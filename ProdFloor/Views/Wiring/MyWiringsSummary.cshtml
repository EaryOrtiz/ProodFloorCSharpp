@model ProdFloor.Models.ViewModels.Wiring.WiringViewModel
@foreach (var j in Model.WiringJobList)
{
    AppUser CurrentUser = UserManager.Users.FirstOrDefault(m => m.EngID == j.WirerID);
    PO po = Model.POList.FirstOrDefault(m => m.POID == j.POID);
    StatusPO statusPO = Model.StatusPOList.FirstOrDefault(m => m.POID == j.POID);
    Job MyJob = Model.JobList.FirstOrDefault(m => m.JobID == po.JobID);

    <div class="card card-outline-primary m-1 p-1">
        <div class="bg-faded p-1">
            <h4>
                Job #@MyJob.JobNum | PO #@po.PONumb
                <span class="badge badge-pill badge-secondary" style="float:right">
                    <small>@statusPO.Status</small>
                </span>
                <span class="badge badge-pill badge-primary" style="float:right">
                    <small>Station @Model.StationsList.FirstOrDefault(m => m.StationID == j.StationID).Label</small>
                </span>
                <span class="badge badge-pill badge-secondary" style="float:right">
                    <small>@Model.JobTypeList.FirstOrDefault(m => m.JobTypeID == MyJob.JobTypeID).Name</small>
                </span>
            </h4>
        </div>
    </div>
}

