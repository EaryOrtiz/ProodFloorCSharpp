            <div class="card-body">
                <div class="row">
                    <div class="col-3 mr-4">
                        <!-- Button trigger to Jobs report Modal-->
                        <button type="button" class="btn-warning btn text-light" data-toggle="modal" data-target="#DailyModal" data-tooltip="tooltip" data-placement="top" title="Generate Daily Report">
                            <i class="fas fa-cloud-sun fa-4x"></i>
                        </button>
                    </div>
                    <div class="col-7">
                        <h4 class="mb-0 text">Daily </h4>
                        <hr class="mt-0 mb-2" />
                        <p>Data to be defined...</p>
                    </div>
                </div>

            </div>
        </div>

        <div class="card text-white bg-success  mb-3 mr-4" style="max-width: 28rem;">
            <div class="card-body">
                <div class="row">
                    <div class="col-3 mr-4">
                        <!-- Button trigger to Efficiency report Modal-->
                        <button type="button" class="btn-success  btn" data-toggle="modal" data-target="#EfficiencyModal" data-tooltip="tooltip" data-placement="top" title="Generate Efficency Report">
                            <i class="fas fa-chart-line  fa-5x"></i>
                        </button>
                    </div>
                    <div class="col-7">
                        <h4 class="mb-0 text">Efficiency</h4>
                        <hr class="mt-0 mb-2" />
                        <p>Data to be defined...</p>
                    </div>
                </div>

            </div>
        </div>

