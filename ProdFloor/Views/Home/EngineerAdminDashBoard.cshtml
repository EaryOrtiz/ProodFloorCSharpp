@model DashboardIndexViewModel
@{
    ViewBag.Title = "Enginneer Admin Dashboard";
    Layout = "_AdminLayoutWO";

}
<br />
@if (User.IsInRole("EngAdmin"))
{
    @if (Model.OnCrossJobs != null)
    {
        @Html.Partial("OngoingJobsEngAdmin", Model)
    }
    <br />
    @if (Model.OnCrossJobs != null)
    {
        @Html.Partial("OngoingCrossApprovalsEngAdmin", Model)
    }
    <br />
    @if (Model.OnCrossJobs != null)
    {
        @Html.Partial("PendingCrossApprovalsEngAdmin", Model)
    }
}