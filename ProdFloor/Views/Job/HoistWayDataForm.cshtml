@model JobViewModel
<input type="hidden" asp-for="CurrentHoistWayData.JobID" />
<input type="hidden" asp-for="CurrentHoistWayData.HoistWayDataID" />
<div class="form-group row has-warning has-feedback">
    <label asp-for="CurrentHoistWayData.Capacity" class="col-sm-2 col-form-label"></label>
    <div>
        <input asp-for="CurrentHoistWayData.Capacity" class="form-control col-sm-7" />
        <span asp-validation-for="CurrentHoistWayData.Capacity" class="text-danger"></span>
    </div>
</div>
<div class="form-group row has-warning has-feedback">
    <label asp-for="CurrentHoistWayData.UpSpeed" class="col-sm-2 col-form-label"></label>
    <div>
        <input asp-for="CurrentHoistWayData.UpSpeed" class="form-control col-sm-7" />
        <span asp-validation-for="CurrentHoistWayData.UpSpeed" class="text-danger"></span>
    </div>
</div>
