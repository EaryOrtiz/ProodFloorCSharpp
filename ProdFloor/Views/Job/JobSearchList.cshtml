<div page-model="@Model.PagingInfo" page-action="JobSearchList" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-secondary"
     page-class-selected="btn-primary" page-url-jobType="@Model.JobsSearchList"
     class="btn-group pull-right m-1">
</div>
@{
    <script type="text/javascript">
        $("#BatterySelect").ready(function () {
            $("#BatteryBrand").hide();
        });

        $(document).ready(function () {
            $("#BatterySelect").change(function () {
                $("select option:selected").each(function () {
                    if ($(this).attr("value") == "No" || ("value") == "") {
                        $("#BatteryBrand").hide();
                    }
                    if ($(this).attr("value") == "Si") {
                        $("#BatteryBrand").show();
                    }
                });
            }).change();
        });
    </script>
}