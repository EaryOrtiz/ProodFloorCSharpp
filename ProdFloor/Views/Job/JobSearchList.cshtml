                    Top Floor: <input type="text" class="form-control form-control-sm col-sm-10" asp-for="TopFloor">
                    Number of Front Floor Openings: <input type="text" class="form-control form-control-sm col-sm-10" asp-for="FrontFloorOpenings">
                    <br />
                    <select asp-for="AnyRear" class="form-control form-control-sm col-sm-7">
                        <option value="">Select a Job with Rear Floor</option>
                        <option value="Si">------------ Si -----------</option>
                        <option value="No">------------ No ----------</option>
                    </select>
                    <br />
                </div>
                <div class="col-sm">
                    <br />
                    <select asp-for="IndependentRearOpenings" class="form-control form-control-sm col-sm-11">
                        <option value="">Select a Job with Independent Rear Openings</option>
                        <option value="Si">--------------------------- Si --------------------------</option>
                        <option value="No">--------------------------- No -------------------------</option>
                    </select>
                    <br />
                    Number of Rear Floor Openings: <input type="text" class="form-control form-control-sm col-sm-10" asp-for="RearFloorOpenings">
