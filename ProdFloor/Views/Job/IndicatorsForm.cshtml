@model JobViewModel
<input type="hidden" asp-for="CurrentIndicator.JobID" />
<input type="hidden" asp-for="CurrentIndicator.IndicatorID" />
<input type="hidden" asp-for="CurrentJob.JobID" />
<input type="hidden" asp-for="CurrentJob.Status" />
<input type="hidden" asp-for="CurrentJob.EngID" />
<h2 class="col-sm-10">
    Calls
</h2>
<div class="form-row col-sm-12" style="padding-bottom:20px">
    <div class="input-group col-5 text-center">
        <div class="input-group-prepend">
            <span class="input-group-text" id="">Car Calls</span>
        </div>
        <indicators select-for="Volts_Value" selected-value="@Model.CurrentIndicator.CarCallsVoltage" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.CarCallsVoltage" class="form-control col-sm-3"></indicators>
        <indicators select-for="Volts_Type" selected-value="@Model.CurrentIndicator.CarCallsVoltageType" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.CarCallsVoltageType" class="form-control col-sm-3"></indicators>
        <indicators select-for="CallType" selected-value="@Model.CurrentIndicator.CarCallsType" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.CarCallsType" class="form-control col-sm-3"></indicators>
        <div>
            <div><span asp-validation-for="CurrentIndicator.CarCallsVoltage" class="text-danger"></span></div>
            <div><span asp-validation-for="CurrentIndicator.CarCallsVoltageType " class="text-danger"></span></div>
            <div><span asp-validation-for="CurrentIndicator.CarCallsType" class="text-danger"></span></div>
        </div>
    </div>
    <div class="input-group col-5 text-center">
        <div class="input-group-prepend">
            <span class="input-group-text" id="">Hall Calls</span>
        </div>
        <indicators select-for="Volts_Value" selected-value="@Model.CurrentIndicator.HallCallsVoltage" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.HallCallsVoltage" class="form-control col-sm-3"></indicators>
        <indicators select-for="Volts_Type" selected-value="@Model.CurrentIndicator.HallCallsVoltageType" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.HallCallsVoltageType" class="form-control col-sm-3"></indicators>
        <indicators select-for="CallType" selected-value="@Model.CurrentIndicator.HallCallsType" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.HallCallsType" class="form-control col-sm-3"></indicators>
        <div>
            <div><span asp-validation-for="CurrentIndicator.HallCallsVoltage" class="text-danger"></span></div>
            <div><span asp-validation-for="CurrentIndicator.HallCallsVoltageType " class="text-danger"></span></div>
            <div><span asp-validation-for="CurrentIndicator.HallCallsType" class="text-danger"></span></div>
        </div>
    </div>
</div>
<h2 class="col-sm-10">
    Position Indicators
</h2>
<div class="form-row col-sm-12" style="padding-bottom:20px">
    <div class="input-group col-5 text-center" style="padding-bottom:5px">
        <div class="input-group-prepend">
            <span class="input-group-text" id="">Car PI</span>
        </div>
        <div><span asp-validation-for="CurrentIndicator.CarPI" class="text-danger"></span></div>
        <div class="input-group-text">
            <input asp-for="CurrentIndicator.CarPI" disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" />
        </div>
        <indicators select-for="PIDriver" selected-value="@Model.CurrentIndicator.CarPIType" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.CarPIType" class="form-control col-sm-3"></indicators>
        <indicators select-for="CarPIDiscreteType" selected-value="@Model.CurrentIndicator.CarPIDiscreteType" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.CarPIDiscreteType" class="form-control col-sm-3"></indicators>
        <div>
            <div><span asp-validation-for="CurrentIndicator.CarPIType" class="text-danger"></span></div>
            <div><span asp-validation-for="CurrentIndicator.CarPIDiscreteType" class="text-danger"></span></div>
        </div>
    </div>
    <div class="input-group col-5 text-center" style="padding-bottom:5px">
        <div class="input-group-prepend">
            <span class="input-group-text" id="">Hall PI</span>
        </div>
        <div><span asp-validation-for="CurrentIndicator.HallPI" class="text-danger"></span></div>
        <div class="input-group-text">
            <input asp-for="CurrentIndicator.HallPI" disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" />
        </div>
        <div><span asp-validation-for="CurrentIndicator.HallPIType" class="text-danger"></span></div>
        <indicators select-for="PIDriver" selected-value="@Model.CurrentIndicator.HallPIType" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.HallPIType" class="form-control col-sm-3"></indicators>
        <div><span asp-validation-for="CurrentIndicator.HallPIDiscreteType" class="text-danger"></span></div>
        <indicators select-for="CarPIDiscreteType" selected-value="@Model.CurrentIndicator.HallPIDiscreteType" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.HallPIDiscreteType" class="form-control col-sm-3"></indicators>
    </div>
    <div class="input-group col-5 text-center">
        <div class="input-group-prepend">
            <span class="input-group-text" id="">Voice Annunciation</span>
        </div>
        <div><span asp-validation-for="CurrentIndicator.VoiceAnnunciationPI" class="text-danger"></span></div>
        <div class="input-group-text">
            <input asp-for="CurrentIndicator.VoiceAnnunciationPI" disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" />
        </div>
        <div><span asp-validation-for="CurrentIndicator.VoiceAnnunciationPIType" class="text-danger"></span></div>
        <indicators select-for="PIDriver" selected-value="@Model.CurrentIndicator.VoiceAnnunciationPIType" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.VoiceAnnunciationPIType" class="form-control col-sm-3"></indicators>
    </div>
</div>
