    <div class="input-group col-5 text-center">
        <div class="input-group-prepend">
            <span class="input-group-text" id="">Voltage</span>
        </div>
        <volts select-for="Volts_Value" selected-value="@Model.CurrentIndicator.IndicatorsVoltage" asp-is-disabled="@(Model.CurrentUserID == Model.CurrentJob.EngID || User.IsInRole("Admin") ? false : true)" asp-for="CurrentIndicator.IndicatorsVoltage" class="form-control col-sm-2"></volts>
